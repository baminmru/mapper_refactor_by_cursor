{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=mapper;User Id=postgres;Password=;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/mapper-.log",
          "rollingInterval": "Day"
        }
      }
    ]
  }
} 